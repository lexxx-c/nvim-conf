
local M = {
    'ldelossa/litee-filetree.nvim',
    dependencies = {
        'ldelossa/litee.nvim',
        'kyazdani42/nvim-web-devicons',
    }
}

M.config = function ()
    require'litee.filetree'.setup{
        panel = {
            orientation = 'right',
            panel_size = 30,
        }
    }
end


return M

